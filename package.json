{
  "name": "kilroy-example",
  "scripts": {
    "postinstall": "cp node_modules/kilroy/dist/kilroy.js public/kilroy.js",
    "build": "cat node_modules/kilroy/dist/kilroy.js public/main.js | uglifyjs -cm > public/scripts.min.js",
    "start": "ecstatic -p 8000 public"
  },
  "dependencies": {
    "kilroy": "^0.1.1"
  },
  "devDependencies": {
    "ecstatic": "~0.8.0",
    "uglifyjs": "^2.4.10"
  }
}
